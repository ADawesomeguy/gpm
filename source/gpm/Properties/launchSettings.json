{
  "profiles": {
    "gpm update": {
      "commandName": "Project",
      "commandLineArgs": "update gpm-org/gpm.installer/wpf -g"
    },
    "gpm list": {
      "commandName": "Project",
      "commandLineArgs": "list"
    },
    "gpm install": {
      "commandName": "Project",
      "commandLineArgs": "install gpm-org/gpm.installer/wpf -g"
    },
    "gpm remove": {
      "commandName": "Project",
      "commandLineArgs": "remove gpm-org/gpm.installer/wpf -s 0"
    },
    "gpm search": {
      "commandName": "Project",
      "commandLineArgs": "search"
    },
    "gpm restore": {
      "commandName": "Project",
      "commandLineArgs": "restore"
    },
    "gpm new": {
      "commandName": "Project",
      "commandLineArgs": "new https://github.com/gpm-org/gpm.Installer.git -o out -i wpf"
    },
    "gpm help": {
      "commandName": "Project",
      "commandLineArgs": "-h"
    },
